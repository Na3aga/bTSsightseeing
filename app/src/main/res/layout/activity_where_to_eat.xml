<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    android:background="@color/colorPrimary"
    tools:context=".mvp.view.main.MainActivity">

    <Button
        android:id="@+id/buttonbeck3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#448AFF"
        android:text=""
        android:drawableTint="@color/colorWhite"
        android:drawableLeft="@mipmap/ic_chevron_left_white_48dp"
   />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="210dp"
        android:id="@+id/restOleksandria"
        android:background="@drawable/restoleksandria"
        android:layout_margin="3dp"

        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="210dp"
        android:id="@+id/restShale"
        android:background="@drawable/restshale"
        android:layout_margin="3dp"

        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="210dp"
        android:id="@+id/torgCenter"
        android:background="@drawable/torgcentr"
        android:layout_margin="3dp"

        />


    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="?attr/colorPrimary"
        app:popupTheme="@style/AppTheme.PopupOverlay"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="0dp"
        tools:layout_conversion_absoluteWidth="411dp"
        tools:layout_conversion_absoluteHeight="56dp" />


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_dialog_email"
/>

</LinearLayout>

