<?xml version="1.0" encoding="utf-8"?>
<com.sothree.slidinguppanel.SlidingUpPanelLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:sothree="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/sliding_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom"
    sothree:umanoPanelHeight="0dp"
    sothree:umanoShadowHeight="4dp">

    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="4"
        tools:context=".mvp.view.map.MapsActivity" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#fdfbfb"
        android:orientation="vertical">


        <android.support.v7.widget.CardView
            android:id="@+id/card_view1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="1dp"

            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/MapsPanelTextViewName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_marginTop="15dp"
                    android:drawableTint="#2e2e2e"
                    android:gravity="center"
                    android:text="Sithseeing Name1"
                    android:textColor="#2e2e2e"
                    android:textSize="22sp" />


            </LinearLayout>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/card_view2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="1dp"

            >

            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <TextView
                    android:id="@+id/MapsPanelTextView11"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:drawableLeft="@mipmap/ic_location_on_black_24dp"
                    android:drawableTint="#2196F3"
                    android:text=""
                    android:textColor="#2e2e2e"
                    android:textSize="20dp"
                    sothree:layout_constraintBottom_toBottomOf="parent"
                    sothree:layout_constraintLeft_toLeftOf="parent"
                    sothree:layout_constraintTop_toTopOf="parent"
                    sothree:layout_constraintVertical_bias="0.333"

                    />

                <TextView
                    android:id="@+id/MapsPanelTextView22"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_marginTop="8dp"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="------"
                    android:textColor="#2e2e2e"
                    android:textSize="18sp"
                    sothree:layout_constraintBottom_toBottomOf="parent"
                    sothree:layout_constraintLeft_toRightOf="@+id/MapsPanelTextView11"
                    sothree:layout_constraintRight_toRightOf="parent"
                    sothree:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>
        </android.support.v7.widget.CardView>


        <android.support.v7.widget.CardView
            android:id="@+id/card_view3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="1dp">

            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/MapsPanelTextView21"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:drawableLeft="@mipmap/ic_phone_black_24dp"
                    android:drawableTint="#2196F3"
                    android:text=""
                    android:textColor="#2e2e2e"
                    android:textSize="20dp"
                    sothree:layout_constraintBottom_toBottomOf="parent"
                    sothree:layout_constraintLeft_toLeftOf="parent"
                    sothree:layout_constraintTop_toTopOf="parent"
                    sothree:layout_constraintVertical_bias="0.333" />

                <TextView
                    android:id="@+id/MapsPanelTextView32"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_marginTop="8dp"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="------"
                    android:textColor="#2e2e2e"
                    android:textSize="18sp"
                    sothree:layout_constraintBottom_toBottomOf="parent"
                    sothree:layout_constraintLeft_toRightOf="@+id/MapsPanelTextView21"
                    sothree:layout_constraintRight_toRightOf="parent"
                    sothree:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/card_view4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="1dp">

            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/MapsPanelTextView31"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:drawableLeft="@mipmap/ic_watch_later_black_24dp"
                    android:drawableTint="#2196F3"
                    android:text=""
                    android:textColor="#2e2e2e"
                    android:textSize="20dp"
                    sothree:layout_constraintBottom_toBottomOf="parent"
                    sothree:layout_constraintLeft_toLeftOf="parent"
                    sothree:layout_constraintTop_toTopOf="parent"
                    sothree:layout_constraintVertical_bias="0.333"

                    />

                <TextView
                    android:id="@+id/MapsPanelTextView42"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_marginTop="8dp"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="------"
                    android:textColor="#2e2e2e"
                    android:textSize="18sp"
                    sothree:layout_constraintBottom_toBottomOf="parent"
                    sothree:layout_constraintLeft_toRightOf="@+id/MapsPanelTextView31"
                    sothree:layout_constraintRight_toRightOf="parent"
                    sothree:layout_constraintTop_toTopOf="parent" />
            </android.support.constraint.ConstraintLayout>
        </android.support.v7.widget.CardView>
    </LinearLayout>


</com.sothree.slidinguppanel.SlidingUpPanelLayout>

